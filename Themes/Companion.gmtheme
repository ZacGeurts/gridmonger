ui {
  window {
    modified-flag {
      normal = "#d2c3a978"
      inactive = "#ffffff40"
    }
    border {
      color = "#1b1b1bff"
    }
    background {
      color = "#b2944eff"
      image = ""
    }
    title {
      background {
        normal = "#3c3222ff"
        inactive = "#292929ff"
      }
      text {
        normal = "#d2c3a9ca"
        inactive = "#ffffff5f"
      }
    }
    button {
      normal = "#d4bf9b79"
      hover = "#d4bf9ba6"
      down = "#d4bf9bcf"
      inactive = "#d4d4d441"
    }
  }

  dialog {
    corner-radius = 6
    background = "#3f382cff"
    label = "#bfbaa4f0"
    warning = "#f2c024ec"
    error = "#ff5e3bff"

    title {
      background = "#242019ff"
      text = "#fffde4dd"
    }
    inner-border {
      color = "#00000000"
      width = 0
    }
    outer-border {
      color = "#00000000"
      width = 0
    }
    shadow {
      enabled = yes
      color = "#0000003b"
      feather = 12
      x-offset = 1
      y-offset = 1
    }
  }

  widget {
    corner-radius = 5

    background {
      normal = "#c9c1a1a8"
      hover = "#c9c1a1dc"
      active = "#e1bc43ff"
      disabled = "#c3bb9c51"
    }
    foreground {
      normal = "#000000b3"
      active = "#000000b3"
      disabled = "#00000066"
    }
  }

  text-field {
    cursor = "#ffbe00ff"
    selection = "#c8820078"

    edit {
      background = "#c9b4873d"
      text = "#ffffffb3"
    }
    scroll-bar {
      normal = "#00000000"
      edit = "#ffffff00"
    }
  }

  status-bar {
    background = "#3e3428ff"
    text = "#f5e3c5ca"
    warning = "#f2c024ec"
    coordinates = "#ad9964eb"

    command {
      background = "#a6823eff"
      text = "#3e3428ff"
    }

    error = "#ff5e3bff"
  }

  about-button {
    label {
      normal = "#00000066"
      hover = "#00000080"
      down = "#00000099"
    }
  }

  about-dialog {
    logo = "#c9b487ff"
  }

  quick-help {
    background = "#504231ff"
    text = "#f5e3c5ca"

    command {
      background = "#c09648ff"
      text = "#3e3428ff"
    }

    title = "#f5e3c598"
  }

  splash-image {
    logo = "#52452fff"
    outline = "#c9b487ff"
    shadow-alpha = 0.55834578043316
  }

  drop-down {
    item-list-background = "#26221aff"
  }
}

level {
  general {
    background = "#b2944eff"
    line-width = normal
    cursor = "#e1bc43ff"
    cursor-guides = "#ffb91e2b"
    link-marker = "#078594d6"
    selection = "#ff808066"
    paste-preview = "#3399ff66"

    trail {
      normal = "#312d336a"
      cursor = "#0f0d0f46"
    }
    foreground {
      normal {
        normal = "#644b14ff"
        cursor = "#644b14ff"
      }
      light {
        normal = "#644b155c"
        cursor = "#644b155c"
      }
    }
    coordinates {
      normal = "#644b14cd"
      highlight = "#624400ff"
    }
    region-border {
      normal = "#ae590dff"
      empty = "#ae590d6f"
    }

    link-line = "#0785949b"
  }

  background-hatch {
    enabled = no
    color = "#00000000"
    width = 1
    spacing-factor = 3
  }

  grid {
    background {
      style = none
      grid = "#00000000"
    }
    floor {
      style = solid
      grid = "#c8a65757"
    }
  }

  outline {
    style = none
    fill-style = solid
    color = "#00000000"
    width-factor = 0
    overscan = no
  }

  shadow {
    inner {
      color = "#00000000"
      width-factor = 0
    }
    outer {
      color = "#00000000"
      width-factor = 0
    }
  }

  floor {
    transparent = no
    background = [
      "#aa8c46ff"
      "#83000025"
      "#d5440043"
      "#ffc12d4b"
      "#483c1874"
      "#4059004a"
      "#6f811b72"
      "#3b94d656"
      "#2a597b6b"
      "#7e45815c"
    ]
  }

  note {
    marker {
      normal = "#3c2c1cbe"
      cursor = "#3c2c1cbe"
    }

    comment = "#ff5f37dc"
    background-shape = circle
    index = "#000000b5"
    index-background = [
      "#ff8e29e8"
      "#52cd93a8"
      "#ffd15ca4"
      "#ff7474d8"
      "#51402eff"
    ]

    tooltip {
      background = "#333333e2"
      text = "#fffbc6e2"
      corner-radius = 5

      shadow {
        color = "#00000064"
      }
    }
  }

  label {
    text = ["#644b14ff", "#ffe7bee7", "#a15207ff", "#ffb93dfb"]
  }

  level-drop-down {
    item-list-background = "#51402eff"
    corner-radius = 5

    button {
      normal = "#ffffff00"
      hover = "#ffffff26"
      label = "#4e3e19ff"
    }
    item {
      normal = "#fffbc6e2"
      hover = "#3c3422e6"
    }
    shadow {
      color = "#00000064"
    }
  }
}

pane {
  current-note {
    text = "#4e3e19ff"
    scroll-bar = "#d3c5b8ff"
    index = "#000000b5"
    index-background = ["#ff8e29e8", "#52cd93a8", "#ffd15ca4", "#ff7474d8"]
  }

  toolbar {
    button {
      normal = "#b2944eff"
      hover = "#c6a557ff"
    }
  }

  notes-list {
    controls-background = "#322d23ff"
    list-background = "#3f382cff"

    item {
      background {
        hover = "#4e483dff"
        active = "#5c564cff"
      }
      text {
        normal = "#bfbaa4f0"
        hover = "#ccc8b6f3"
        active = "#d9d6c8f6"
      }
    }
    level-section {
      background = "#2c271fff"
      text = "#ccc8b6f3"
    }
    region-section {
      background = "#1f1c15ff"
      text = "#ccc8b6f3"
    }

    section-separator = "#514839ff"
    scroll-bar = "#9f9c96ff"
  }
}

